{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6",
      "es2016",
      "DOM",
      "DOM.Iterable",
      "ESNext",
      "esnext.asynciterable"
    ],
    "skipLibCheck": true,
    "jsxImportSource": "@emotion/react",
    "baseUrl": "./src",
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "module": "es2020",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "target": "esnext",
    "jsx": "react-jsx",
    "outDir": "./dist"
  },
  "include": ["src"]
}
